          <% courseListData.forEach(college => { %>
           
           
              <div class="col-md-6">

                <div class="CollegeMobileList_list shadow mt-3  ">
                  <div class="CollegeMobileList_listData">
                    <div class="row">
                      <div class="col-3">
                        <div class="image">
                          <img src="<%= college.logoSrc %>" alt="logo" />
                        </div>
                      </div>
                      <div class="col-9">
                        <div class="CollegeMobileList_data">
                        
                            <p class="border px-2 mb-1"><%= college.category%></p>
                       
                          <h6><%= college.name %></h6>
                          <ul>
                            <li class="d-flex align-items-center">
                              <p>( <%= college.rating %> )&nbsp; </p>
                              <div>
                                <% for (let i = 0; i < college.star; i++) { %>
                                  <i class="bi bi-star-fill"></i>
                                <% } %>
                              </div>
                              <p>&nbsp;&nbsp; <%= college.reviewCount %></p>
                            </li>
                            <li>
                       
                              <div><p> <%= college.overview %></p></div>
                            </li>
                          </ul>
                        </div>
                      </div>
                    </div>
