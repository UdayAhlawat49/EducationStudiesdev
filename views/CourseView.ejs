<%- include('partials/header') %>

  <% console.log("courseViewData" ,courseViewData) %>
    <!-- course hero + tab links start -->
    <section class="conatiner-fluid section-padding">
      <div class="container course-hero">
        <div class="row">
          <div class="col">

            <div class="course-hero-content">

              <h3>
                <%= courseViewData.collegeDetails.name %>
              </h3>
              <p>
                <%= courseViewData.collegeDetails.description %>
              </p>

            </div>

          </div>
        </div>
        <hr>
        <div class="row">
          <div class="col">
            <div class="course-tabs">
              <ul class="nav nav-tabs border-bottom-0" id="myTab" role="tablist">
                <li class="nav-item  " role="presentation">
                  <button class="nav-link active" id="overview-tab" data-bs-toggle="tab" data-bs-target="#overview"
                    type="button" role="tab" aria-controls="overview" aria-selected="true">Overview</button>
                </li>
                <li class="nav-item" role="presentation">
                  <button class="nav-link" id="syllabus-tab" data-bs-toggle="tab" data-bs-target="#syllabus"
                    type="button" role="tab" aria-controls="syllabus" aria-selected="false">Syllabus</button>
                </li>
                <li class="nav-item" role="presentation">
                  <button class="nav-link" id="carrer-tab" data-bs-toggle="tab" data-bs-target="#carrer" type="button"
                    role="tab" aria-controls="carrer" aria-selected="false">Carrer</button>
                </li>
                <li class="nav-item" role="presentation">
                  <button class="nav-link" id="admission-tab" data-bs-toggle="tab" data-bs-target="#admission"
                    type="button" role="tab" aria-controls="admission" aria-selected="false">Admission</button>
                </li>
                <li class="nav-item" role="presentation">
                  <button class="nav-link" id="popular-colleges-tab" data-bs-toggle="tab"
                    data-bs-target="#popular-colleges" type="button" role="tab" aria-controls="popular-colleges"
                    aria-selected="false">Popular Colleges</button>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- course hero + tab links end -->

    <!-- tab content start -->
    <section class="container-fluid section-padding">
      <div class="container course-tab-section">
        <div class="row">
          <div class="col">
            <div class="tab-content" id="myTabContent">
              <!-- Overview tab data start  -->
              <div class="tab-pane fade show active" id="overview" role="tabpanel" aria-labelledby="overview-tab">
                <% console.log("overviewTabData" , courseViewData) %>
                 <!-- ===================================================overviewTabData=========================================================================================== -->

                  <% if(courseViewData.overviewTabData  ) { %>
                    <!-- Course Detail Table start-->
                    <div class="course-table">
                      <div class="course-table-content">
                        <h4>
                          <%= courseViewData.overviewTabData.type %>
                        </h4>
                        <p>
                          <%= courseViewData.overviewTabData.content %>
                        </p>
                      </div>
                      <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover align-middle">


                          <thead>
                            <tr>
                              <th>Particulars</th>
                              <th>MBA Course Details </th>
                            </tr>
                          </thead>
                          <tbody>
                            <% courseViewData.overviewTabData.Particularstable.forEach( tableData=> { %>
                              <tr>
                                <td>
                                  <%= tableData.Particulars %>
                                </td>
                                <td>
                                  <%= tableData.CourseDetails %>
                                </td>
                              </tr>
                              <% }) %>


                          </tbody>
                        </table>
                      </div>
                    </div>
                    <!-- Courses Eligibility Criteria Table start -->

                    <div class="course-table">
                      <div class="course-table-content">
                        <h4>
                          <%= courseViewData.overviewTabData.Eligibility.heading %>
                        </h4>
                        <p>
                          <%= courseViewData.overviewTabData.Eligibility.content%>
                        </p>
                      </div>

                      <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover">
                          <thead>
                            <tr>
                              <th>Course Type</th>
                              <th>Eligibility Criteria</th>
                            </tr>
                          </thead>
                          <tbody>
                            <% courseViewData.overviewTabData.CourseTypeTable.forEach( tableData=> { %>
                              <tr>
                                <td>
                                  <%= tableData.CourseType %>
                                </td>
                                <td>
                                  <%= tableData.EligibilityCriteria %>
                                </td>
                              </tr>
                              <% }) %>

                          </tbody>
                        </table>
                      </div>
                      <!-- Course Exam Table start -->
                      <div class="course-table">
                        <div class="course-table-content">
                          <h4>
                            <%= courseViewData.overviewTabData.EntranceExams.heading %>
                          </h4>
                          <p>
                            <%= courseViewData.overviewTabData.EntranceExams.content %>
                          </p>
                        </div>

                        <div class="table-responsive ">
                          <table class="table table-striped table-bordered table-hover align-middle">
                            <thead>
                              <tr>
                                <th>Entrance Exams</th>
                                <th>Exam Dates</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% courseViewData.overviewTabData.EntranceTable.forEach( tableData=> { %>
                                <tr>
                                  <td>
                                    <%= tableData.EntranceExams %>
                                  </td>
                                  <td>
                                    <%= tableData.ExamDates %>
                                  </td>
                                </tr>
                                <% }) %>

                            </tbody>
                          </table>
                        </div>
                      </div>
                      <!-- Course CutOff table start -->
                      <div class="course-table">
                        <div class="course-table-content">
                          <h4>
                            <%= courseViewData.overviewTabData.Cutoff.heading %>
                          </h4>
                          <p>
                            <%= courseViewData.overviewTabData.Cutoff.content %>
                          </p>
                        </div>

                        <div class="table-responsive">
                          <table class="table table table-striped table-bordered table-hover align-middle">
                            <thead>
                              <tr>
                                <th class="table-head">IIMs</th>
                                <th class="table-head">CAT Cutoff 2023 (Percentile)</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% courseViewData.overviewTabData.IIMsTable.forEach( tableData=> { %>
                                <tr>
                                  <td>
                                    <%= tableData.IIMs %>
                                  </td>
                                  <td>
                                    <%= tableData.Percentile %>
                                  </td>
                                </tr>
                                <% }) %>

                                  <!-- Add more rows as needed -->
                            </tbody>
                          </table>
                        </div>
                    </div>

                  <% } %>

           






                    </div>
              </div>
              <!-- overview tab data end  -->
              <!-- syllabus Tab data start  -->
              <div class="tab-pane fade" id="syllabus" role="tabpanel" aria-labelledby="syllabus-tab">

                       <!-- ===================================================Syllabus=========================================================================================== -->

                       <% if(courseViewData.syllabusTabContentData) { %>

                        
                <!-- course syllabus  start -->
                <div class="course-overview-content">
                  <h4><%= courseViewData.syllabusTabContentData.title %></h4>
                  <hr>
                  <p><%= courseViewData.syllabusTabContentData.content %></p>
                </div>
                <!-- option 1 -->
                <!-- Course core subject start-->
                <div class="course-table">
                  <div class="course-table-content">
                    <h4>Lists of Core Subjects in MBA</h4>
                    <p>Given below are tables indicating the lists of core and elective subjects included in the MBA
                      syllabus.</p>
                  </div>

                  <!-- Core subjects in MBA -->
                  <div class="table-responsive">
                    <table class="table table table-striped table-bordered table-hover align-middle">
                      <thead>
                        <tr>
                          <th>MBA Subjects</th>
                          <th>Details</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% courseViewData.syllabusTabContentData.coreSubjecttable.forEach( tableData=> { %>
                          <tr>
                            <td>
                              <%= tableData.Subject %>
                            </td>
                            <td>
                              <%= tableData.SubjectDetails %>
                            </td>
                          </tr>
                          <% }) %>
                        <tr>
                           
                        </tr>
                        <!-- Add more rows as needed -->
                      </tbody>
                    </table>
                  </div>

                  <!-- Add more tables for other subjects as needed -->
                </div>

                <% } %>
                <!-- option 2 -->
                <!-- Course elective  subject start-->
                <div class="course-table">
                  <div class="course-table-content">
                    <h4>Lists of Subjects in MBA</h4>
                    <p>Given below are tables indicating the lists of core and elective subjects included in the MBA
                      syllabus.</p>
                  </div>

                  <!-- Core subjects in MBA -->
                  <div class="table-responsive">
                    <table class="table table table-striped table-bordered table-hover align-middle">
                      <thead>
                        <tr>
                          <th>MBA Subjects</th>
                          <th>Details</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tbody>
                          <% courseViewData.syllabusTabContentData.selectedSubjecttable.forEach( tableData=> { %>
                            <tr>
                              <td>
                                <%= tableData.Subject %>
                              </td>
                              <td>
                                <%= tableData.SubjectDetails %>
                              </td>
                            </tr>
                            <% }) %>
                        
                          <!-- Add more rows as needed -->
                        </tbody>
                        <!-- Add more rows as needed -->
                      </tbody>
                    </table>
                  </div>

                  <!-- Add more tables for other subjects as needed -->
                </div>


              </div>
              <!-- syllabus Tab data end  -->

              <!-- Carrer tab data start -->
              <div class="tab-pane fade" id="carrer" role="tabpanel" aria-labelledby="carrer-tab">
               
                  <!-- ===================================================Carrer=========================================================================================== -->

                  <% if(courseViewData.carrerTabContentData) { %>

               
                    <div class="course-carrer-content">
                  <h4><%= courseViewData.carrerTabContentData.title %></h4>
                  <hr>
                  <% courseViewData.carrerTabContentData.content.forEach(content => { %>
                    <li>
                      <p><%= content %></p>
                    </li>
                  <% }) %>
                
                 
                </div>

                <% } %>   
                <!-- course top carrer start-->
                <div class="course-table">
                  <div class="course-table-content">
                    <h4>Top MBA Careers</h4>
                    <p>According to AIMS, banking, finance, consulting, marketing, manufacturing and technology are some
                      of the fields that are craving for MBA graduates. When it comes to actual job titles, here is a
                      list of some of the most in-demand jobs:</p>
                  </div>

                  <!-- top cerrer with job profile and sallary table start -->
                  <div class="table-responsive">
                    <table class="table table table-striped table-bordered table-hover align-middle">
                      <thead>
                        <tr>
                          <th>Job Profiles</th>
                          <th>Average Salary (in INR)</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% courseViewData.carrerTabContentData.Careerstable.forEach( tableData=> { %>
                          <tr>
                            <td>
                              <%= tableData.Profiles %>
                            </td>
                            <td>
                              <%= tableData.Salary %>
                            </td>
                          </tr>
                          <% }) %>

                        
                        <!-- Add more rows as needed -->
                      </tbody>
                    </table>
                  </div>

                  <!-- Add more tables for other subjects as needed -->
                </div>

              </div>
              <!-- Carrer tab data end -->

              <!-- admission tab data start -->
              <div class="tab-pane fade" id="admission" role="tabpanel" aria-labelledby="admission-tab">
                <div class="course-content">
                  <h4><%= courseViewData.admissionTabContentData.title %></h4>
                  <hr>
                  <% courseViewData.admissionTabContentData.content.forEach(content => { %>
                    <li>
                      <p><%= content %></p>
                    </li>
                  <% }) %>
                
                 
                </div>

                <!-- addmision with Entrance Exams and date table start -->
                <div class="table-responsive">
                  <table class="table table table-striped table-bordered table-hover align-middle">
                    <thead>
                      <tr>
                        <th>Entrance Exams</th>
                        <th>Exam Date</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% courseViewData.admissionTabContentData.admissiontable.forEach( tableData=> { %>
                        <tr>
                          <td>
                            <%= tableData.Exams %>
                          </td>
                          <td>
                            <%= tableData.Date %>
                          </td>
                        </tr>
                        <% }) %>
                      <!-- Add more rows as needed -->
                    </tbody>
                  </table>
                </div>
              </div>
              <!-- admission tab data end -->


              <div class="tab-pane fade" id="popular-colleges" role="tabpanel" aria-labelledby="popular-colleges-tab">
                Popular Colleges</div>
            </div>
          </div>
        </div>
      </div>


    </section>
    <!-- tab content start -->

    <%- include('partials/footer') %>