      <div class="row">
        <h4 class="  bold">
          Best <%= category %> Colleges in Decemeber ( 2023 )
        </h4>
      </div>
      <div class="row my-4 mx-auto">
        <div class="col-md-9">
          <div class="row">
       
             <!-- <% console.log("currentPage" ,currentPage) %>  -->
             <!-- <% console.log("collegeListData" ,collegeListData) %>  -->
            <% collegeListData.forEach(college => { %>

            <div class="col-md-6">
              <div class="CollegeMobileList_list shadow mt-3">
                <div class="CollegeMobileList_listData">
                  <div class="row">
                    <div class="col-3">
                      <div class="image">
                        <img
                          src="<%= college.collegeDetails.logoSrc %>"
                          alt="logo"
                        />
                      </div>
                    </div>
                    <div class="col-9">
                      <div class="CollegeMobileList_data">
                        <% if (college.collegeDetails.topRated) { %>
                        <p class="border px-2 mb-1">
                          <i class="bi bi-star-fill"></i> &nbsp; Top Rated
                        </p>
                        <% } %>
                        <!-- <% console.log("college.collegeDetails.name" ,college.collegeDetails.name) %>  -->

                        <h6><%= college.collegeDetails.name %></h6>
                        <ul>
                          <li class="d-flex align-items-center">
                            <p>
                              ( <%= college.collegeDetails.rating %> )&nbsp;
                            </p>
                            <div>
                              <% for (let i = 0; i <
                              college.collegeDetails.star; i++) { %>
                              <i class="bi bi-star-fill"></i>
                              <% } %>
                            </div>
                            <p>
                              &nbsp;&nbsp; <%=
                              college.collegeDetails.reviewCount %>
                            </p>
                          </li>
                          <li>
                            <div><i class="bi bi-geo-alt-fill"></i></div>
                            <div>
                              <p><%= college.collegeDetails.address %></p>
                            </div>
                          </li>
                        </ul>
                      </div>
                    </div>
                  </div>
                  <div class="row mt-4">
                    <div class="col-6">
                      <div class="callNow">
                        <button>
                          <a href="tel"
                            ><i class="bi bi-telephone-fill"></i> &nbsp; Call
                            Now</a
                          >
                        </button>
                      </div>
                    </div>
                    <div class="col-6">
                      <div class="Enquiry">
                        <button>
                          <a href="/collegeView/<%= college.collegeCode %>"
                            >Learn More</a
                          >
                        </button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <% }); %>
          </div>

          <div class="row my-5">
            <nav aria-label="Page navigation example">
              <ul class="pagination justify-content-center">
          
                
                <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
                  <a class="page-link" href="/collegeslist/<%= category %>/<%= prevPage %>"   tabindex="-1" aria-disabled="true">Previous</a>
                </li>
          
                <% for (let page = 1; page <= totalPages; page++) { %>

                  <li class="page-item <%= currentPage === page ? 'active' : '' %>">
                    <a class="page-link" href="/collegeslist/<%= category %>/<%= page %>"   ><%= page %></a>
                  </li>
                <% } %>
                
          
                <li class="page-item <%= currentPage === totalPages ? 'disabled' : '' %>">
                  <a class="page-link"  href="/collegeslist/<%= category %>/<%= nextPage %>"   >Next</a>
                </li>
          
              </ul>
            </nav>
          
        </div>
       
      </div>
      <div class="col-md-3 mt-3 d-none d-md-block">
        <div class="blog_right_sidebar shadow-lg">
          <aside class="single_sidebar_widget search_widget">
            <form action="#">
              <div class="form-group">
                <div class="input-group mb-3">
                  <input
                    type="text"
                    class="form-control"
                    placeholder="Search Keyword"
                    onfocus="this.placeholder = ''"
                    onblur="this.placeholder = 'Search Keyword'"
                  />
                  <div class="input-group-append">
                    <button class="btn" type="button">
                      <i class="ti-search"></i>
                    </button>
                  </div>
                </div>
              </div>
              <button
                class="button rounded-0 primary-bg text-white w-100 btn_1 boxed-btn"
                type="submit"
              >
                Search
              </button>
            </form>
          </aside>

          <aside class="single_sidebar_widget post_category_widget">
            <h5 class="widget_title bold">Category</h5>
            <ul class="list cat-list">
              <li>
                <a href="/collegeslist/engineering/1" class="d-flex">
                  <p>Engineering</p>
                </a>
              </li>
              <li>
                <a href="/collegeslist/management/1" class="d-flex">
                  <p>Managment</p>
                </a>
              </li>
              <li>
                <a href="/collegeslist/medical/1" class="d-flex">
                  <p>Medical</p>
                </a>
              </li>
            </ul>
          </aside>

          <aside class="single_sidebar_widget popular_post_widget">
            <h5 class="widget_title bold">Popural Colleges</h5>
            <% collegeListDataByTag.forEach(college => { %>

            <div class="media post_item">
              <img width="40" src="./assets/images/lokpok_.webp" alt="post" />
              <div class="media-body">
                <a href="single-blog.html">
                  <h5 class="bold"><%= college.collegeDetails.name %></h5>
                </a>
                <p><%= college.collegeDetails.website %></p>
              </div>
            </div>

            <% }) %>
          </aside>
          <aside class="single_sidebar_widget tag_cloud_widget">
            <h5 class="widget_title bold">Courses</h5>
            <ul class="list">
              <li>
                <a href="#">B.tech</a>
              </li>
              <li>
                <a href="#">MBA</a>
              </li>
              <li>
                <a href="#">MBBS</a>
              </li>
              <li>
                <a href="#">BSC</a>
              </li>
              <li>
                <a href="#">BCA</a>
              </li>
              <li>
                <a href="#">B.Com</a>
              </li>
              <li>
                <a href="#">MSC</a>
              </li>
              <li>
                <a href="#">BBA</a>
              </li>
            </ul>
          </aside>

          <aside class="single_sidebar_widget instagram_feeds">
            <h5 class="widget_title bold">Featured Colleges</h5>
            <ul class="instagram_row d-flex">
              <li>
                <a href="#">
                  <img
                    width="50"
                    class="img-fluid"
                    src="./assets/images/lokpok_.webp"
                    alt=""
                  />
                </a>
              </li>
              <li>
                <a href="#">
                  <img
                    width="50"
                    class="img-fluid"
                    src="./assets/images/lokpok_.webp"
                    alt=""
                  />
                </a>
              </li>
              <li>
                <a href="#">
                  <img
                    width="50"
                    class="img-fluid"
                    src="./assets/images/lokpok_.webp"
                    alt=""
                  />
                </a>
              </li>
              <li>
                <a href="#">
                  <img
                    width="50"
                    class="img-fluid"
                    src="./assets/images/lokpok_.webp"
                    alt=""
                  />
                </a>
              </li>
            </ul>
          </aside>

          <aside class="single_sidebar_widget newsletter_widget">
            <h5 class="widget_title bold">Newsletter</h5>

            <form action="#">
              <div class="form-group">
                <input
                  type="email"
                  class="form-control"
                  onfocus="this.placeholder = ''"
                  onblur="this.placeholder = 'Enter email'"
                  placeholder="Enter email"
                  required
                />
              </div>
              <button
                class="button rounded-0 primary-bg text-white w-100 btn_1 boxed-btn"
                type="submit"
              >
                Subscribe
              </button>
            </form>
          </aside>
        </div>
      </div>
    </div>
  </div>
</div>
